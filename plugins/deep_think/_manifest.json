{
  "manifest_version": 1,
  "name": "Deep Think插件 (Deep Think Actions)",
  "version": "1.0.0",
  "description": "可以深度思考",
  "author": {
    "name": "SengokuCola",
    "url": "https://github.com/MaiM-with-u"
  },
  "license": "GPL-v3.0-or-later",
  
  "host_application": {
    "min_version": "0.11.0"
  },
